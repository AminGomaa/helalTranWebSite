@model Helalzahbi_Trans.Models.Job

@{
    ViewBag.Title = "بيانات الخدمات";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div>
    <hr />
    <dl class="dl-horizontal">
        
        <dd>
            @Html.DisplayFor(model => model.category.categName)
        </dd>

        <dd>
            @Html.DisplayFor(model => model.jobtitl)
        </dd>

        <dd>
            @Html.DisplayFor(model => model.jobcontent)
        </dd>

        <dd>
          <img src="~/Uploads/@Html.DisplayFor(model => model.jobimage)" class="thumbnail" style="width:150px;height:60px;"/>
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("تعديل", "Edit", new { id = Model.id }) |
    @Html.ActionLink("العودة للقائمة", "Index")
</p>
